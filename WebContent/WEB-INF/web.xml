<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <welcome-file-list>
    <welcome-file>stuadd.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>GBKFilter</filter-name>
    <filter-class>com.filter.GBKFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>GBKFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>myServlet</servlet-name>
    <servlet-class>com.servlet.myServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myServlet</servlet-name>
    <url-pattern>/servlet/myServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>picServlet</servlet-name>
    <servlet-class>com.servlet.picServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>picServlet</servlet-name>
    <url-pattern>/servlet/picServlet</url-pattern>
  </servlet-mapping>
</web-app>